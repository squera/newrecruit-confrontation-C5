            <characteristic name="Fervore" typeId="b8fb-8240-e49d-a802"/>
            <characteristic name="Difficolta`" typeId="356a-95af-2838-f908">6</characteristic>
            <characteristic name="Area" typeId="607c-b4e2-dea9-c7ad">s</characteristic>

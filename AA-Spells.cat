        <selectionEntry id="2e67-08ad-970b-b722" name="Torpore eterno" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1c4e-05b5-e04b-5875" type="max"/>

                <characteristic name="Difficulty" typeId="74a3e70b-f6ea-9986-411a-6f42221e9cd7">8</characteristic>
                <characteristic name="Frequence" typeId="553afca1-843a-ea5d-e5e7-226d1317ca22">1</characteristic>
                <characteristic name="Duration" typeId="290e8e09-179a-a022-9453-4a9255541189">UER</characteristic>

            <profile id="be73-a85d-4a0c-2e63" name="Descrizione" hidden="false" typeId="59c438c5-9c9d-77ab-a8f2-44fc0593263c" typeName="Spell Card">
              <characteristics>
                <characteristic name="Intensity" typeId="a5ee0c5a-2235-cc84-42f3-e3320860e983">4</characteristic>
                <characteristic name="Mana Cost" typeId="a7f17c01-e84c-882c-96f2-5d7f9af96438">3 tenebre</characteristic>
